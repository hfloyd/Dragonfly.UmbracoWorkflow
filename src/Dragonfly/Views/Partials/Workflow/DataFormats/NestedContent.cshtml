@using Umbraco.Core.Models
@using Umbraco.Web
@model Umbraco.Core.Models.IPublishedProperty

@{
    var ncItems = Model.GetValue<IEnumerable<IPublishedContent>>().ToList();

    if (ncItems.Any())
    {
        <ol>
            @foreach (var item in ncItems)
            {
                <li>@item.Name</li>
            }
        </ol>
    }
    else
    {
        <i>No items added</i>
    }
}


